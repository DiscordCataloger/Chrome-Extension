{
  "manifest_version": 3,
  "name": "News Corner",
  "description": "Get updated news anytime, anywhere.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "placeholder.png"
  },
  "content_scripts": [
    {
      "js": ["script.js", "flowbite.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
